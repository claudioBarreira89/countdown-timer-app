{
    "compilerOptions": {
        "outDir": "./dist/",
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "moduleResolution": "node",
        "strictNullChecks": false
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
